# Environment Variables for Data Collector

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Ollama Configuration (optional - defaults to localhost)
OLLAMA_HOST=http://localhost:11434

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Collection Settings
RATE_LIMIT_DELAY=1.0  # Seconds between OSM requests
BATCH_SIZE=100        # Items to process in one batch
PARALLEL_PROVINCES=5  # Provinces to process simultaneously
